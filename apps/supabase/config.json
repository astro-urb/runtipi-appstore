{
  "name": "Supabase",
  "available": true,
  "port": 8000,
  "exposable": true,
  "dynamic_config": true,
  "id": "supabase",
  "description": "The open source Firebase alternative with instant APIs, Edge Functions, realtime subscriptions, authentication, and storage",
  "tipi_version": 3,
  "version": "latest",
  "categories": ["database", "development", "storage"],
  "short_desc": "Open source Firebase alternative",
  "author": "Supabase",
  "source": "https://github.com/supabase/supabase",
